# Minimal OS variable loading for performance
. /etc/profile.d/02-distribution

tocon "Restoring volume..."

VOLUME=$(get_setting audio.volume)
if [[ -z ${VOLUME} ]]
then
  VOLUME="60"
fi

/usr/bin/volume ${VOLUME}

